{
  "manifest_version": 2,
  "name": "txt-web",
  "version": "1.1",

  "description": "A simple engine designed for a plaintext internet.",

  "icons": {
    "48": "icons/t-48.png",
    "96": "icons/t-96.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*/*.txt"],
      "js": ["txt-web.js"],
      "css": ["css/base.css"]
    }
  ]
}
